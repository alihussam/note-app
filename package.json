{
  "name": "note-app",
  "version": "1.0.0",
  "description": "Note taking application",
  "main": "./build/index.js",
  "engines": {
    "node": "20.17.0"
  },
  "engineStrict": true,
  "scripts": {
    "prebuild": "rimraf \"build\"",
    "build": "tsc",
    "start": "node ./build/src/index.js",
    "dev": "env-cmd -f .env.local ts-node-dev --respawn --transpile-only ./src/index.ts",
    "seed": "env-cmd -f .env.local ts-node-dev --respawn --transpile-only ./scripts/seedDb.ts"
  },
  "author": "Hussam <contact.alihussam@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "5.1.1",
    "express": "4.21.1",
    "express-validation": "4.1.1",
    "http-status-codes": "2.3.0",
    "ioredis": "5.4.1",
    "jsonwebtoken": "9.0.2",
    "lodash": "4.17.21",
    "moment": "2.30.1",
    "sequelize": "6.37.5",
    "sqlite3": "5.1.7"
  },
  "devDependencies": {
    "@types/bcrypt": "5.0.2",
    "@types/express": "5.0.0",
    "@types/jsonwebtoken": "9.0.7",
    "@types/lodash": "4.17.13",
    "env-cmd": "10.1.0",
    "ts-node-dev": "2.0.0",
    "typescript": "5.6.3"
  }
}
